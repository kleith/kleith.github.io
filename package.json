    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "dompurify": "^3.0.5",
    "i18next": "^23.5.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.2",
    "styled-components": "^6.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
